{
  "name": "superdebug",
  "version": "2.2.0",
  "description": "Debug Superagent Requests - generates curl output",
  "main": "superdebug-server.js",
  "browser": "superdebug-browser.js",
  "scripts": {
    "lint": "standard",
    "fix": "standard --fix",
    "test": "DEBUG=super-* tape test/*.js",
    "coverage": "nyc --reporter=html --reporter=text npm run test"
  },
  "keywords": [
    "log",
    "debug",
    "superagent",
    "curl"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andineck/superdebug"
  },
  "author": "Andi Neck",
  "license": "MIT",
  "devDependencies": {
    "lodash": "^4.17.11",
    "form-data": "^2.3.1",
    "nock": "^9.0.22",
    "nyc": "^10.1.2",
    "superagent": "^3.6.3",
    "standard": "^12.0.1",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "debug": "^2.6.0"
  }
}
